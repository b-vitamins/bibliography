@inproceedings{tevet2025closd,
  abstract = {Motion diffusion models and Reinforcement Learning (RL) based control for physics-based simulations have complementary strengths for human motion generation. The former is capable of generating a wide variety of motions, adhering to intuitive control such as text, while the latter offers physically plausible motion and direct interaction with the environment. We present CLoSD, a method that combines motion diffusion and reinforcement learning for multi-task character control. The key insight is that motion diffusion can serve as an on-the-fly universal planner for a robust RL controller. CLoSD maintains a closed-loop interaction between two modules: a Diffusion Planner (DiP), and a tracking controller.},
  archiveprefix = {arXiv},
