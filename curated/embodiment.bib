@article{lee2017soft,
  abstract = {Soft robots are often inspired from biological systems which consist of soft materials or are actuated by electrically activated materials. There are several advantages of soft robots compared to the conventional robots; safe human-machine interaction, adaptability to wearable devices, simple gripping system, and so on. Due to the unique features and advantages, soft robots have a considerable range of applications. This article reviews state-of-the-art researches on soft robots and application areas. Actuation systems for soft robots can be categorized and analyzed into three types: variable length tendon, fluidic actuation, and electro-active polymer systems.},
  author = {Lee, C. and Kim, M. and Kim, Y. J. and Hong, N. and Ryu, S. and Kim, H. J. and Kim, S.},
  doi = {10.1007/s12555-016-0462-3},
  file = {:/home/b/documents/article/lee2017soft.pdf:pdf},
  journal = {International Journal of Control, Automation, and Systems},
  number = {1},
  pages = {3--15},
  pdf = {https://link.springer.com/content/pdf/10.1007/s12555-016-0462-3.pdf},
  publisher = {Springer},
  title = {Soft Robot Review},
  url = {https://link.springer.com/article/10.1007/s12555-016-0462-3},
  volume = {15},
  year = {2017}
}

@article{rus2015design,
  abstract = {Conventionally, engineers have employed rigid materials to fabricate precise, predictable robotic systems, which are easily modelled as rigid members connected at discrete joints. Natural systems, however, often match or exceed the performance of robotic systems with deformable bodies. Cephalopods, for example, achieve amazing feats of manipulation and locomotion without a skeleton; even vertebrates such as humans achieve dynamic gaits by storing elastic energy in their compliant bones and soft tissues. Inspired by nature, engineers have begun to explore the design and control of soft-bodied robots composed of compliant materials. This Review discusses recent developments in the emerging field of soft robotics.},
  author = {Rus, Daniela and Tolley, Michael T.},
  doi = {10.1038/nature14543},
  file = {:/home/b/documents/article/rus2015design.pdf:pdf},
  journal = {Nature},
  month = {5},
  number = {7553},
  pages = {467--475},
  pdf = {https://dspace.mit.edu/bitstream/handle/1721.1/100772/SoftRoboticsReview-FinalAuthorVersion.pdf?sequence=1&isAllowed=y},
  publisher = {Nature Publishing Group},
  title = {Design, Fabrication and Control of Soft Robots},
  url = {https://www.nature.com/articles/nature14543},
  volume = {521},
  year = {2015}
}

@inproceedings{robinson1999continuum,
  abstract = {Like the human limbs which inspired them most robots are discrete mechanisms with rigid links connected by single degree of freedom joints. In contrast, 'continuum' and 'serpentine' robot mechanisms move by bending through a series of continuous arcs producing motion which resembles that of biological tentacles or snakes. This paper provides a single reference to the expanding technology of continuum robot mechanisms. It defines the fundamental difference between discrete, serpentine and continuum robot devices, presents the 'state of the art' of continuum robots, outlines their areas of application, and introduces some control issues. Finally, some conclusions regarding the continued development of these devices are made.},
  address = {Detroit, MI, USA},
  author = {Robinson, G. and Davies, J. B. C.},
  booktitle = {Proceedings of the 1999 IEEE International Conference on Robotics and Automation},
  doi = {10.1109/ROBOT.1999.774029},
  organization = {IEEE},
  pages = {2849--2854},
  pdf = {https://ieeexplore.ieee.org/iel5/6243/16742/00774029.pdf},
  publisher = {IEEE},
  title = {Continuum Robots---A State of the Art},
  url = {https://ieeexplore.ieee.org/document/774029/},
  volume = {4},
  year = {1999}
}

@article{websteriii2010design,
  abstract = {Continuum robots represent a growing class of robots that have gained significant attention due to their inherent compliance and ability to operate in constrained environments. This paper provides a review of the existing approaches for design and kinematic modeling of continuum robots that can be characterized by constant curvature bending. The review covers the fundamental mechanical design principles, kinematic modeling approaches, and control methods used for these robots. The paper begins by reviewing various design approaches for continuum robots, including those based on biomimicry, those using traditional materials, and those employing novel smart materials. The kinematic modeling approaches are then reviewed, focusing on methods that assume constant curvature bending along the length of the robot.},
  author = {Webster III, Robert J. and Jones, Bryan A.},
  doi = {10.1177/0278364910368147},
  journal = {The International Journal of Robotics Research},
  number = {13},
  pages = {1661--1683},
  pdf = {https://www.semanticscholar.org/paper/Design-and-Kinematic-Modeling-of-Constant-Curvature-Webster-Jones/f0dfe127a576737d3e28313defaccf763fca18b4},
  publisher = {SAGE Publications},
  title = {Design and Kinematic Modeling of Constant Curvature Continuum Robots: A Review},
  url = {https://journals.sagepub.com/doi/10.1177/0278364910368147},
  volume = {29},
  year = {2010}
}

@article{zhang2022survey,
  abstract = {In this paper, we describe the advances in the design, actuation, modeling, and control field of continuum robots. After decades of pioneering research, many innovative structural design and actuation methods have arisen. Untethered magnetic robots are a good example; its external actuation characteristic allows for miniaturization, and they have gotten a lot of interest from academics. Furthermore, continuum robots with proprioceptive abilities are also studied. In modeling, modeling approaches based on continuum mechanics and geometric shaping hypothesis have made significant progress after years of research. Machine learning also shows potential in robotics. In this survey, we review the design, actuation, modeling, and control methods of continuum robots.},
  articleno = {9754697},
  author = {Zhang, Jingyu and Fang, Qin and Xiang, Pingyu and Sun, Danying and Xue, Yanan and Jin, Rui and Qiu, Ke and Xiong, Rong and Wang, Yue and Lu, Haojian},
  doi = {10.34133/2022/9754697},
  journal = {Cyborg and Bionic Systems},
  month = {7},
  pdf = {https://cdn.ncbi.nlm.nih.gov/pmc/articles/PMC11014731/pdf/2022_9754697.pdf},
  publisher = {American Association for the Advancement of Science},
  title = {A Survey on Design, Actuation, Modeling, and Control of Continuum Robot},
  url = {https://spj.science.org/doi/10.34133/2022/9754697},
  volume = {2022},
  year = {2022}
}

@article{caluwaerts2014design,
  abstract = {To better understand the role of tensegrity structures in biological systems and their application to robotics, the Dynamic Tensegrity Robotics Lab at NASA Ames Research Center has developed and validated two software environments for the analysis, simulation and design of tensegrity robots. These tools, along with new control methodologies and the modular hardware components developed to validate them, are presented as a system for the design of actuated tensegrity structures. The paper focuses on spherical tensegrity structures and presents multiple novel control approaches for mobility and terrain interaction that have been tested in simulation. A hardware prototype of a spherical six-bar tensegrity, the Reservoir Compliant Tensegrity Robot, is used to empirically validate the accuracy of simulation.},
  articleno = {20140520},
  author = {Caluwaerts, Ken and Despraz, Jérémie and Işçen, Atil and Sabelhaus, Andrew P. and Bruce, Jonathan and Schrauwen, Benjamin and SunSpiral, Vytas},
  doi = {10.1098/rsif.2014.0520},
  journal = {Journal of the Royal Society Interface},
  month = {9},
  number = {98},
  pages = {20140520},
  pdf = {https://www.ncbi.nlm.nih.gov/pmc/articles/instance/4233701/bin/rsif20140520.pdf},
  publisher = {The Royal Society},
  title = {Design and Control of Compliant Tensegrity Robots Through Simulation and Hardware Validation},
  url = {https://royalsocietypublishing.org/doi/10.1098/rsif.2014.0520},
  volume = {11},
  year = {2014}
}

@article{shah2022tensegrity,
  abstract = {Numerous recent advances in robotics have been inspired by the biological principle of tensile integrity—or "tensegrity"—to achieve remarkable feats of dexterity and resilience. Tensegrity robots contain compliant networks of rigid struts and soft cables, allowing them to change their shape by adjusting their internal tension. Numerous techniques have been proposed for designing and simulating tensegrity robots, giving rise to a wide range of locomotion modes, including rolling, vibrating, hopping, and crawling. In this study, we review progress in the burgeoning field of tensegrity robotics, highlighting several emerging challenges, including automated design, state sensing, and kinodynamic motion planning.},
  author = {Shah, Dylan S. and Booth, Joran W. and Baines, Robert L. and Wang, Kun and Vespignani, Massimo and Bekris, Kostas and Kramer-Bottiglio, Rebecca},
  doi = {10.1089/soro.2020.0170},
  file = {:/home/b/documents/article/shah2022tensegrity.pdf:pdf},
  journal = {Soft Robotics},
  month = {8},
  note = {Epub 2021 Oct 27},
  number = {4},
  pages = {639--656},
  pdf = {https://www.eng.yale.edu/faboratory/publications/journal/2021/Shah%20et%20al.%20-%202021%20-%20Tensegrity%20Robotics.pdf},
  publisher = {Mary Ann Liebert},
  title = {Tensegrity Robotics},
  url = {https://www.liebertpub.com/doi/10.1089/soro.2020.0170},
  volume = {9},
  year = {2022}
}

@article{strgar2025accelerated,
  abstract = {The co-design of robot morphology and neural control typically requires using reinforcement learning to approximate a unique control policy gradient for each body plan, demanding massive amounts of training data to measure the performance of each design. We show that a universal, morphology-agnostic controller can be rapidly and directly obtained by gradient-based optimization through differentiable simulation. This process of morphological pretraining allows the designer to explore non-differentiable changes to a robot's physical layout (e.g., adding, removing and recombining discrete body parts) and immediately determine which revisions are beneficial and which are deleterious using the pretrained model. We demonstrate this approach by conducting what we call zero-shot evolution and compare it with the simultaneous co-optimization of a universal controller alongside an evolving design population.},
  archiveprefix = {arXiv},
  author = {Strgar, Luke and Kriegman, Sam},
  eprint = {2502.10862},
  file = {:/home/b/documents/article/strgar2025accelerated.pdf:pdf},
  journal = {arXiv preprint arXiv:2502.10862},
  note = {Submitted to arXiv on February 15, 2025},
  pdf = {https://arxiv.org/pdf/2502.10862.pdf},
  title = {Accelerated Co-design of Robots Through Morphological Pretraining},
  url = {https://arxiv.org/abs/2502.10862},
  year = {2025}
}

@article{mo2024design,
  abstract = {This study proposes two tensegrity hopping robots actuated by push-pull electromagnets and servo motors respectively. Both tensegrity robots are able to conduct stable and consecutive hopping actions. The research covers structural designs, theoretical modeling of the hopping actuators, and detailed analysis of the robot's self-righting properties, all validated by corresponding experimental and simulational results. The first hopping robot could hop forward at an average speed of 0.641 body length/s, while the second robot has a lower moving speed of 0.237 body length/s but its average jumping height of 0.301 m is nearly 2.5 times higher than that of the first robot. Compared with other tensegrity rolling robots, the proposed two robots show obvious advantages in locomotion performance, making them suitable for applications in space exploration, urban search, military surveillance, and environmental detection.},
  author = {Mo, Jixue and Fang, Hao and Yang, Qingkai},
  doi = {10.1016/j.isci.2024.109226},
  issn = {2589-0042},
  journal = {iScience},
  month = {3},
  note = {Published online February 16, 2024},
  number = {3},
  pages = {109226},
  pdf = {https://pmc.ncbi.nlm.nih.gov/articles/PMC10910242/pdf/main.pdf},
  pmcid = {PMC10910242},
  publisher = {Elsevier},
  title = {Design and Locomotion Characteristic Analysis of Two Kinds of Tensegrity Hopping Robots},
  url = {https://www.sciencedirect.com/science/article/pii/S2589004224004474},
  volume = {27},
  year = {2024}
}

@article{jani2014review,
  abstract = {Shape memory alloys (SMAs) belong to a class of shape memory materials that have the ability to memorise or retain their previous form when subjected to certain stimulus such as thermomechanical or magnetic variations. SMAs have drawn significant attention and interest in recent years in a broad range of commercial applications, due to their unique and superior properties including pseudoelasticity, high actuation stress (400-700 MPa), high actuation strain (ca. 8%), high energy density (ca. 1200 J/kg), and three-dimensional actuation capabilities. This paper provides a comprehensive review of recent SMA research and commercial applications, categorizing over 100 state-of-the-art patents against relevant commercial domains including automotive, aerospace, robotic and biomedical applications. The review addresses SMA attributes that make them ideally suited to actuators and discusses their associated limitations to clarify design challenges faced by SMA developers.},
  author = {Jani, Jaronie Mohd and Leary, Martin and Subic, Aleksandar and Gibson, Mark A.},
  doi = {10.1016/j.matdes.2013.11.084},
  issn = {0261-3069},
  journal = {Materials & Design},
  keywords = {Shape memory alloys, Actuators, Smart materials, Applications, Design challenges},
  note = {Comprehensive review with over 100 patents analyzed across commercial domains},
  pages = {1078--1113},
  pdf = {https://www.semanticscholar.org/paper/A-review-of-shape-memory-alloy-research,-and-Jani-Leary/64b9869ec451a7883e4735e56dada90deb6b1b2d},
  publisher = {Elsevier},
  title = {A Review of Shape Memory Alloy Research, Applications and Opportunities},
  url = {https://www.sciencedirect.com/science/article/abs/pii/S0261306913011345},
  volume = {56},
  year = {2014}
}

@inproceedings{barcohen1999electroactive,
  abstract = {NASA is seeking to reduce the mass, size, consumed power, and cost of the instrumentation used in future missions. Electroactive polymers (EAP) offer an effective alternative to current actuators. Two families of EAP materials are being investigated: bending ionomers and longitudinal electrostatically driven elastomers. These materials were demonstrated to effectively actuate manipulation devices. Bending-EAP operation was observed at conditions that exceed the harsh environment on Mars. An effective coating technique using sprayed polymer was developed, extending EAP operation in air from a few minutes to about four months by forming the equivalent of a skin to protect the moisture content of the ionomer. The team constructed an EAP-driven miniature robotic arm controlled by MATLAB code to drop and lift the arm and close and open EAP fingers of a 4-finger gripper.},
  address = {Newport Beach, CA, USA},
  author = {Bar-Cohen, Yoseph and Leary, Sean P. and Shahinpoor, Mohsen and Harrison, Joycelyn S. and Smith, Joseph G.},
  booktitle = {Smart Structures and Materials 1999: Electroactive Polymer Actuators and Devices},
  doi = {10.1117/12.349708},
  file = {:/home/b/documents/inproceedings/barcohen1999electroactive.pdf:pdf},
  month = {5},
  note = {Cited by 58 scholarly publications},
  organization = {Society of Photo-Optical Instrumentation Engineers},
  pages = {57--63},
  pdf = {https://ntrs.nasa.gov/api/citations/20000056613/downloads/20000056613.pdf},
  publisher = {SPIE},
  series = {Proceedings of SPIE},
  title = {Electroactive Polymer (EAP) Actuators for Planetary Applications},
  url = {https://www.spiedigitallibrary.org/conference-proceedings-of-spie/3669/1/Electroactive-polymer-EAP-actuators-for-planetary-applications/10.1117/12.349708.short},
  volume = {3669},
  year = {1999}
}

@article{barcohen2002electroactive,
  abstract = {Electroactive polymers (EAP) are an emerging class of functional materials that respond to electrical stimulation with large displacement. This attractive characteristic earned them the name artificial muscles. The paper presents a comprehensive review of electroactive polymer materials and their potential applications as actuators. Even though the actuation force and robustness of existing EAP materials require further improvement, there has already been a series of reported successes. EAPs are smart soft materials capable of changing sizes and/or shapes in direct response to electrical stimuli, allowing the combination of actuator, sensor and structure in one single topology. The review covers the current state of EAP technology and discusses challenges and opportunities for future development.},
  author = {Bar-Cohen, Yoseph},
  doi = {10.2514/2.3902},
  file = {:/home/b/documents/article/barcohen2002electroactive.pdf:pdf},
  institution = {Jet Propulsion Laboratory, California Institute of Technology},
  issn = {0022-4650},
  journal = {Journal of Spacecraft and Rockets},
  keywords = {Electroactive polymers, Artificial muscles, Actuators, Smart materials, Spacecraft},
  month = {11},
  note = {Highly cited foundational review paper in electroactive polymer field},
  number = {6},
  pages = {822--827},
  pdf = {https://ntrs.nasa.gov/api/citations/20000056613/downloads/20000056613.pdf},
  publisher = {American Institute of Aeronautics and Astronautics},
  title = {Electroactive Polymers as Artificial Muscles: A Review},
  url = {https://arc.aiaa.org/doi/10.2514/2.3902},
  volume = {39},
  year = {2002}
}

@inproceedings{pratt1995series,
  abstract = {This paper challenges the traditional approach of making the interface between an actuator and its load as stiff as possible. We propose that reducing interface stiffness offers advantages including greater shock tolerance, lower reflected inertia, more accurate and stable force control, less inadvertent damage to the environment, and the capacity for energy storage. Series elastic actuators place linear springs intentionally in series between the motor and actuator output, where spring strain is measured to get an accurate estimate of force. The control system servos the motor to reduce the difference between the desired force and the measured force signal. Test results are presented from a revolute series-elastic actuator designed for the arms of the MIT humanoid robot Cog and for a small planetary rover.},
  address = {Pittsburgh, PA, USA},
  author = {Pratt, Gill A. and Williamson, Matthew M.},
  booktitle = {Proceedings of the 1995 IEEE/RSJ International Conference on Intelligent Robots and Systems: Human Robot Interaction and Cooperative Robots},
  doi = {10.1109/IROS.1995.525827},
  institution = {MIT Leg Laboratory, Massachusetts Institute of Technology},
  keywords = {Series elastic actuators, Force control, Compliance, Robotics, Human-robot interaction},
  note = {Foundational paper introducing series elastic actuator concept; highly cited work},
  pages = {399--406},
  pdf = {https://fab.cba.mit.edu/classes/865.15/people/rebecca.kleinberger/assets/papers/SEA_Pratt.pdf},
  publisher = {IEEE},
  title = {Series Elastic Actuators},
  url = {https://ieeexplore.ieee.org/document/525827/},
  volume = {1},
  year = {1995}
}

@inproceedings{tsagarakis2009compact,
  abstract = {This paper presents the development of a new compact soft actuation unit intended to be used in multi degree of freedom and small scale robotic systems such as the child humanoid robot iCub. Compared to other existing series elastic linear or rotary implementations, the proposed design shows high integration density and wider passive deflection. The miniaturization of the newly developed high performance unit was achieved with the use of a new rotary spring module based on a novel arrangement of linear springs. The mechanical realization of the soft actuation unit is based on the series elastic actuator concept, providing safe human-robot interaction capabilities while maintaining high performance in a compact form factor suitable for small-scale humanoid robots.},
  address = {Kobe, Japan},
  author = {Tsagarakis, Nikos G. and Laffranchi, Matteo and Vanderborght, Bram and Caldwell, Darwin G.},
  booktitle = {Proceedings of the 2009 IEEE International Conference on Robotics and Automation},
