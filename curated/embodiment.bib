}

@inproceedings{khatib2008unified,
  abstract = {This paper presents a unified hierarchical whole-body control framework for humanoid robots addressing physical interactivity challenges. The framework extends previous methods to create control architectures with advanced capabilities and interactive skills for robots operating in human environments. Tasks are hierarchically ranked and torques for lower-priority tasks are projected onto the null-space of the Jacobian matrix associated with higher-priority tasks, enabling multi-contact scenarios with multiple constraints.},
