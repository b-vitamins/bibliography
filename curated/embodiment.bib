}

@article{jiahao2024knowledgebased,
  abstract = {Soft robots have many advantages over rigid robots thanks to their compliant and passive nature. However, it is generally challenging to model the dynamics of soft robots due to their high spatial dimensionality, making it difficult to use model-based methods to accurately control soft robots. This paper proposes KNODE-Cosserat, a framework that combines first-principle physics models and neural ordinary differential equations to leverage the generalization ability of physics-based models and the fast speed of deep learning methods. We adopt knowledge-based neural ordinary differential equations (KNODE) to model soft robot dynamics, using NODE to improve spatial derivative accuracy and applying the method to soft robot systems based on the Cosserat rod model.},
  archiveprefix = {arXiv},
  author = {Jiahao, Tom Z. and Adolf, Ryan and Sung, Cynthia and Hsieh, M. Ani},
