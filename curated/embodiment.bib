}

@inproceedings{cheng2024expressive,
  abstract = {Expressive motions are an important communication medium for humans and are important for humanoid robots to be socially intelligent. This work proposes ExBody, a method for whole-body control of humanoid robots that can generate rich, diverse, and expressive motions. The key insight is to train a policy that can track reference motions from large-scale human motion capture datasets using reinforcement learning. However, directly training on motion capture data presents challenges due to the mismatch between human and robot morphology and physical capabilities. ExBody addresses this by encouraging the upper body to imitate the reference motion while allowing the legs to deviate to maintain stability and follow velocity commands. Through training in simulation and sim-to-real transfer, the resulting policy enables a humanoid robot to walk in different styles, shake hands with humans, and even dance in the real world.},
  address = {Delft, Netherlands},
  archiveprefix = {arXiv},
  author = {Cheng, Xuxin and Ji, Yandong and Chen, Junming and Yang, Ruihan and Yang, Ge and Wang, Xiaolong},
