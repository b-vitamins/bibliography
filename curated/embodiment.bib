  abstract = {This seminal work established the foundational concept of central pattern generators (CPGs) for locomotion. Brown demonstrated that the cat spinal cord can generate a locomotor rhythm in the absence of input from higher centers and afferent feedback, proposing the 'half-center' model of spinal CPG with two groups of spinal neurons reciprocally organized and mutually inhibiting each other, capable of producing the basic rhythm and pattern for stepping.},
  author = {Brown, T. Graham},
  doi = {10.1113/jphysiol.1914.sp001646},
  journal = {The Journal of Physiology},
  keywords = {central pattern generator, locomotion, half-center oscillator, spinal cord, motor control},
  month = {3},
  note = {Historic paper establishing the CPG concept that revolutionized understanding of motor control},
  number = {1},
  pages = {18--46},
  pdf = {https://pmc.ncbi.nlm.nih.gov/articles/PMC1420503/pdf},
  title = {On the nature of the fundamental activity of the nervous centres; together with an analysis of the conditioning of rhythmic activity in progression, and a theory of the evolution of function in the nervous system},
