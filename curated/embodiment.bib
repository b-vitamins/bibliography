  eprint = {2410.21229},
  file = {:/home/b/documents/inproceedings/he2025hover.pdf:pdf},
  note = {Note: Original entry had different arXiv ID (2404.05831) and different author list. Corrected to match actual HOVER paper.},
  pdf = {https://hover-versatile-humanoid.github.io/resources/HOVER_paper.pdf},
