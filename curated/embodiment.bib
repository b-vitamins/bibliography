  type = {S.M. in Mechanical Engineering},
  url = {https://dspace.mit.edu/handle/1721.1/118671},
  year = {2018}
}

@article{dewang2025research,
  abstract = {Soft robots, constructed from deformable materials, offer significant advantages over rigid robots by mimicking biological tissues and providing enhanced adaptability, safety, and functionality across various applications. Central to these robots are electroactive polymer (EAP) actuators, which allow large deformations in response to external stimuli. This review examines various EAP actuators, including dielectric elastomers, liquid crystal elastomers (LCEs), and ionic polymers, focusing on their potential as artificial muscles.},
