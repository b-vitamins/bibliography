  publisher = {IEEE},
  title = {A Compact Soft Actuator Unit for Small Scale Human Friendly Robots},
  url = {https://ieeexplore.ieee.org/document/5152496},
  year = {2009}
}

@mastersthesis{katz2018low,
  abstract = {This thesis details the hardware and control development for a low-cost modular actuator, intended for use in highly dynamic robots. A small 12 degree of freedom quadruped robot has built using these actuators, on which several control experiments have been performed. Despite the relatively low cost of the actuators, the quadruped has demonstrated unprecedented dynamic behaviors for a robot of this scale and number of degrees of freedom, such as a full 360° backflip from standing on flat ground.},
  advisor = {Sangbae Kim},
  author = {Katz, Benjamin G.},
  file = {:/home/b/documents/mastersthesis/katz2018low.pdf:pdf},
