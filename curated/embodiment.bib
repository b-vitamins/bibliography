  number = {3},
  pages = {147--159},
  pdf = {https://www.cs.cmu.edu/~hgeyer/Teaching/R16-899B/Papers/TagaEA91BiolCybern.pdf},
  title = {Self-organized control of bipedal locomotion by neural oscillators in unpredictable environment},
  url = {https://link.springer.com/article/10.1007/BF00204049},
