  url = {https://hover-versatile-humanoid.github.io/},
  year = {2025}
}

@article{talbot2025continuoustime,
  abstract = {Accurate, efficient, and robust state estimation is more important than ever in robotics as the variety of platforms and complexity of tasks continue to grow. Discrete-time filters and smoothers have historically been the dominant approach. However, the paradigm of continuous-time state estimation proposes an alternative strategy by estimating variables that express the state as a continuous function of time, which can be evaluated at any query time. This approach not only benefits downstream tasks such as planning and control, but also significantly increases estimator performance and flexibility, as well as reduces sensor preprocessing and interfacing complexity. Despite their advantages, continuous-time methods remain underutilized, potentially because they are less well-known within robotics. This work presents a unifying formulation of these methods and the most exhaustive literature review to date.},
  archiveprefix = {arXiv},
  author = {Talbot, William and Nubert, Julian and Tuna, Turcan and Cadena, Cesar and Dümbgen, Frederike and Tordesillas, Jesus and Barfoot, Timothy D. and Hutter, Marco},
  eprint = {2411.03951},
  file = {:/home/b/documents/article/talbot2025continuoustime.pdf:pdf},
  journal = {IEEE Transactions on Robotics},
  note = {Submitted to IEEE Transactions on Robotics},
  pdf = {https://arxiv.org/pdf/2411.03951.pdf},
  title = {Continuous-Time State Estimation Methods in Robotics: A Survey},
  url = {https://arxiv.org/abs/2411.03951},
  year = {2025}
}

@article{liu2025discretetime,
  abstract = {Hybrid dynamical systems, which include continuous flow and discrete mode switching, can model robotics tasks like legged robot locomotion. Model-based methods usually depend on predefined gaits, while model-free approaches lack explicit mode-switching knowledge. Here, we introduce a discrete hybrid automata framework to online reinforcement learning for switching among different modes. In our framework, we decouple the identification and control of hybrid systems by introducing a Multi-critic architecture, where each critic targets one mode, and the mode-switching is determined by a beta distribution. To validate our approach, we focus on underactuated quadruped robot skateboarding, which requires coordination of stance and flight phases. The robot successfully learns to skateboard in simulation and demonstrates Sim2Real transfer, validating our hybrid learning approach for complex dynamic tasks.},
  archiveprefix = {arXiv},
  author = {Liu, Hang and Teng, Sangli and Liu, Ben and Zhang, Wei and Ghaffari, Maani},
  eprint = {2503.01842},
  file = {:/home/b/documents/article/liu2025discretetime.pdf:pdf},
  journal = {arXiv preprint arXiv:2503.01842},
  note = {Note: Original entry had incorrect title and authors. Corrected to match actual arXiv paper.},
  pdf = {https://arxiv.org/pdf/2503.01842.pdf},
  title = {Discrete-Time Hybrid Automata Learning: Legged Locomotion Meets Skateboarding},
  url = {https://arxiv.org/abs/2503.01842},
  year = {2025}
}

@book{thomas1981illusion,
  abstract = {Written by two of Disney's famous Nine Old Men, this seminal book provides a comprehensive history of Disney animation and explains the processes involved in creating animated films. The work presents the fundamental principles of animation in clear, non-technical terms and has become known as the animator's bible. The book covers the evolution of Disney animation from early experiments to feature films, documenting the artistic and technical innovations that established Disney as the leader in animation. Through detailed illustrations and behind-the-scenes insights, the authors share their decades of experience in bringing drawings to life.},
