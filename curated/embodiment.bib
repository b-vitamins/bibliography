@inproceedings{righetti2006programmable,
  abstract = {This paper presents a system of coupled nonlinear oscillators to be used as programmable central pattern generators for humanoid robot locomotion. The key innovations include learning rhythmic signals through adaptive differential equations, embedded learning within the dynamical system, ability to modulate locomotion speed, capability to generate backward walking, and integration of sensory feedback to increase gait stability. The system can encapsulate rhythmic trajectories and demonstrate online trajectory generation for a simulated humanoid robot.},
