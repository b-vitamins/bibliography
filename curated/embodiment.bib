@article{gu2025humanoid,
  abstract = {Humanoid robots hold great potential to perform various human-level skills, involving unified locomotion and manipulation in real-world settings. This survey offers a comprehensive overview of the state-of-the-art in humanoid locomotion and manipulation (HLM), focusing on control, planning, and learning methods. We review model-based methods that have been the backbone of humanoid robotics for the past three decades, and examine emerging learning-based methods, with emphasis on reinforcement and imitation learning that enhance the robustness and versatility of loco-manipulation skills. We also assess the potential of integrating foundation models with humanoid embodiments and highlight the emerging role of tactile sensing as a crucial modality for contact-rich interactions.},
  archiveprefix = {arXiv},
