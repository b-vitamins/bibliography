@article{traut2024interactive,
  abstract = {This work proposes a novel approach to enable social robots to interact autonomously with humans in a realistic, engaging, and expressive manner. The approach leverages the 12 Principles of Animation as a well-established framework that animators use to create convincing, dynamic, and emotionally expressive movements. The authors use Dynamic Movement Primitives (DMPs) to implement key animation principles, providing a learnable, explainable, modulable, online adaptable and composable model for automatic expressive motion generation. DMPs enable modulation of the intensities of individual principles and facilitate the decomposition of complex, expressive motion sequences into learnable and parametrizable primitives. Experiments conducted in simulation, on three different robots, and through a user study demonstrate that the approach allows for creating diverse and nuanced expressions using a single base model.},
  author = {Traut, F. L. and Thuruthel, T. G. and Ramos, J. P. G. and Neto, F. L. F. and Paik, J. K.},
