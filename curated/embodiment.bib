@article{huang2024emotion,
  abstract = {Non-verbal cues such as facial expressions, gestures, and body movements play a crucial role in effective interpersonal interactions. Despite the advancements in robotic behaviors, existing methods often fall short in mimicking the diversity and subtlety of human non-verbal communication. This paper introduces a framework, called EMOTION, for generating expressive motion sequences in humanoid robots, enhancing their ability to engage in human-like non-verbal communication. Our approach leverages the in-context learning capability of large language models to dynamically generate socially appropriate gesture motion sequences for human-robot interaction. We conducted comprehensive user studies with 24 participants, evaluating both the naturalness and understandability of the generated motions. Our results demonstrate that motions generated using our framework are perceived as more natural and understandable compared to baseline methods. Additionally, we show that our approach can generate diverse motion sequences and adapt to different social contexts and user preferences.},
  archiveprefix = {arXiv},
  author = {Huang, Peide and Hu, Yuhan and Nechyporenko, Nataliya and Kim, Daehwa and Talbott, Walter and Zhang, Jian},
  eprint = {2410.23234},
  file = {:/home/b/documents/article/huang2024emotion.pdf:pdf},
  journal = {arXiv preprint arXiv:2410.23234},
  keywords = {humanoid robotics, motion generation, in-context learning, large language models, non-verbal communication, human-robot interaction},
  note = {Note: Original entry had different author list. Corrected to match actual arXiv paper.},
  pdf = {https://arxiv.org/pdf/2410.23234.pdf},
